

public class Exp {
	
	
	public static void main(String[] args) {
		System.out.println(reverse(321));
	}
	
	public static int reverse(int x) {
		int res = 0;
		
		while (x != 0) {
			int tail = x % 10;
			int newR = res*10 + tail;
			res = newR;
			x /= 10;
		}
		return res;
		
	}
	
}